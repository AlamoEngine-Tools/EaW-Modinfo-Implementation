{
  "$id": "https://AlamoEngine-Tools.github.io/schemas/3.0.0/lang-info",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Represents the level of localization support for a language.",
  "type": "object",
  "properties": {
    "code": {
      "type": "string",
      "minLength": 2,
      "maxLength": 2
    },
    "support": {
      "type": "number",
      "minimum": 1,
      "maximum": 7
    }
  },
  "required": [
    "code"
  ],
  "additionalProperties": false
}