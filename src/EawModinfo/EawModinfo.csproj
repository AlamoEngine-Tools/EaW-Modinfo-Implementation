<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
    <LangVersion>8.0</LangVersion>
    <Nullable>enable</Nullable>
    <Version>2.1.0</Version>
    <PackageId>eaw.modinfo</PackageId>
    <Authors>AnakinSklavenwalker</Authors>
    <Company>Republic at War Modding Team</Company>
    <Product>EaW Modinfo</Product>
    <Description>Reference implementation of the eaw.modinfo specification which is located at https://github.com/AlamoEngine-Tools/eaw.modinfo</Description>
    <Copyright>2020 - Anakin Sklavenwalker</Copyright>
    <RepositoryType>git</RepositoryType>
    <PackageTags>petroglyph, alamo, plyphx, foc, eaw</PackageTags>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <AssemblyName>Eaw.Modinfo</AssemblyName>
    <AssemblyVersion>2.1.0.0</AssemblyVersion>
    <FileVersion>2.1.0.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.VisualStudio.Validation" Version="15.5.31" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="NuGet.Versioning" Version="5.7.0" />
  </ItemGroup>

</Project>
